/*翻页*/
i{
    color: black;

}
#turnPage{
    text-align: center;
    height: 10%;
    font-size: 12px;
}
#turnPage div{
    margin-top: 2.5%;
}
/*小图片轮播翻页键*/
.leftBtn{
    background-color: rgba(255, 255, 255, 0.8);
    height: 30px;
    width: 30px;
    border-radius: 100%;
    text-align: center;
    padding: 6px;
    display: inline-block;
    margin-top: 45%;
    margin-left: 15px;
    opacity: 0;
    transition: all .5s;
    cursor: pointer;
}
.rightBtn{
    background-color: rgba(255, 255, 255, 0.8);
    height: 30px;
    width: 30px;
    border-radius: 100%;
    text-align: center;
    padding: 6px;
    display: inline-block;
    float: right;
    margin-top: 45%;
    margin-right: 15px;
    opacity: 0;
    transition: all .5s;
    cursor: pointer;
}
.smImg:hover .leftBtn{
    opacity: 1;
}
.smImg:hover .rightBtn{
    opacity: 1;
}
.smImg1:hover .leftBtn{
    opacity: 1;
}
.smImg1:hover .rightBtn{
    opacity: 1;
}
.smText{
    border: 1px rgba(255, 255, 255, 0) solid;
}
/*回到顶部*/
#backTop{
    position: fixed;
    background-color: rgba(255, 255, 255, 0.71);
    border: 1px rgba(0, 0, 0, 0.25) solid;
    border-radius: 100%;
    height: 30px;
    width: 30px;
    bottom: 50px;
    text-align: center;
    line-height: 30px;
    display: none;
}