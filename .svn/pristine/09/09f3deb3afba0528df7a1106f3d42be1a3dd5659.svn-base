<script>
    window.onload=function () {
        console.log($("#dd").text());
        www();
    }

    function www() {
        $.ajax({
            type: "post",
            url: "/gouwu.do",
            data: {//传参
                id:$("#dd").text()
            },
            success: function (data) {
                document.getElementById("ta").innerHTML = ""
                for (var i = 0; i < data.length; i++) {
                    document.getElementById("ta").innerHTML += "<tr>\n" +
                        "                <td>\n" +
                        "                    <img src=\""+data[i].img+"\" alt=\"\">\n" +
                        "                    &nbsp;&nbsp; &nbsp;&nbsp;<span>"+data[i].p_name+"</span></td>\n" +
                        "                <td>"+data[i].p_color+"</td>\n" +
                        "                <td>"+data[i].p_size+"</td>\n" +
                        "                <td>\n" +
                        "                    <select>\n" +
                        "                        <option value=\"0\">1</option>\n" +
                        "                        <option value=\"1\">2</option>\n" +
                        "                    </select>\n" +
                        "                </td>\n" +
                        "                <td><a  class=\"xianWu\">删除</a></td>\n" +
                        "                <td>￥"+data[i].p_price+"</td>\n" +
                        "            </tr>"
                }

            }
        })
    }



</script>
