.read{
    color: black;
    position: relative;
    padding-bottom: 1px;
    cursor: pointer;
    text-decoration: none;
}
.read:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 1px;
    width: 100%;
    height: 1px;
    background: #313131;
    -webkit-transform: scaleX(1);
    transform: scaleX(1);
    opacity: .6;
    -webkit-transform-origin: left;  //原点向左
transform-origin: left;
    transition: -webkit-transform .7s cubic-bezier(.19,1,.22,1) .2s;
    transition: transform .7s cubic-bezier(.19,1,.22,1) .2s;
    transition: transform .7s cubic-bezier(.19,1,.22,1) .2s,-webkit-transform .7s cubic-bezier(.19,1,.22,1) .2s;
}
.read:hover:after {
    -webkit-transform: scaleX(0);
    transform:scaleX(0);
    -webkit-transform-origin: right;
    transform-origin: right;
    transition-delay:0s ;
}
.read:hover:before {
    transition-delay:.3s ;
}
.xianWu{
    color: black;
    position: relative;
    padding-bottom: 2px;
    cursor: pointer;
    text-decoration: none;
    /*display: block;*/
    vertical-align:middle;  /*//中间对齐*/
    overflow:hidden;
    line-height: 2;
}
.xianWu:before{
    content: '';
    position: absolute;
    bottom: 1px;
    left: 0;
    width: 100%;
    height: 1px;
    background: #313131;
    opacity: .6;
    -webkit-transform: scaleX(0);
    transform: scaleX(0);
    -webkit-transform-origin: right;
    transform-origin: right;
    transition: -webkit-transform .9s cubic-bezier(.19,1,.22,1);
    transition: transform .9s cubic-bezier(.19,1,.22,1);
    transition: transform .9s cubic-bezier(.19,1,.22,1),-webkit-transform .9s cubic-bezier(.19,1,.22,1);
}
.xianWu:hover:before{
    -webkit-transform: scaleX(1);
    transform:scaleX(1);
    -webkit-transform-origin: left;
    transform-origin: left;
    /*transition-delay:1s ;*/
}

