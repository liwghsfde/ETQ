.demo1 {
    font-size: 16px;
    color: black;
    position: relative;
}
/* linkone */
.link-1 a.demo1:after {
    content: '';
    border-bottom: solid 1px black;
    position: absolute;
    bottom: 0;
    width: 0;
}


.link-1 a.demo1:after {
    right: 0;
}

.link-1 a.demo1:hover:after {
    width: 100%;
}

.link-1 a.demo1:after {
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}
