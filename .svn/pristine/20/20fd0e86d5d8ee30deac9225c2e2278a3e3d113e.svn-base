*{
    margin: 0;
    padding: 0;
}
section{
    margin-top: 100px;
    margin-bottom: 100px;
    height: 1200px;
    /*border: 1px solid red;*/
    position: relative;
}
hr{
    width: 50%;
}
.myDivLeft{
    width: 25%;
    height: 800px;
    /*background-color: #a685ca;*/
    float: left;
    z-index: 999;
}
.myDivRight{
    width: 75%;
    height: 800px;
    /*background-color: #336633;*/
    float: left;
}
.myDivRight_1{
    width: 40%;
    height: 320px;
    /*background-color: #faa937;*/
    float: left;
}
.myDivRight_2{
    width: 60%;
    height: 320px;
    /*background-color: #0B85CC;*/
    float: left;
}
.myDivLeft_1{
    margin-left: 10%;
}
.myDivLeft_1 h1{
    font-size: 18px;
    font-weight: normal;
}
.myDivLeft_2{
    margin-top: 10%;
    margin-left: 10%;
}
.myDivLeft_2 p{
    font-size: 14px;
    cursor: pointer;
    margin-top: 5px;
}
.myDivRight_1 h1{
    font-size: 18px;
    font-weight: normal;
}
.myDivRight_1 p{
    margin-top: 9%;
    margin-bottom: 5%;
    font-size: 14px;
}
.myDivRight_1 a{
    font-size: 14px;
    color: black;
}
a:hover{
    text-decoration: none;
}
.myDivRight_2 h1{
    font-size: 18px;
    font-weight: normal;
    margin-bottom: 6%;
}
.myDivRight_2 p{
    font-size: 14px;

}
.divP{
    margin-bottom: 3.5%;
}
.myDivRight_2 a{
    font-size: 14px;
    color: black;
}