*{
    margin: 0;
    padding: 0;
}
.divLogo{
    width: 100%;
}
.divLogo h1{
    font-size: 40px;
    text-align: center;
    font-weight: normal;
}
.divSS{
    width: 70%;
    height: 500px;
    /*border: 1px solid red;*/
    margin: 0 auto;
    margin-top: 40px;
}
.divSS h3{
    font-size: 16px;
    margin-top: 20px;
    margin-bottom: 20px;


}
.clsInit{
    font-size: 14px;
    font-family: 微软雅黑;
}
.clsInit select{
    width: 150px;
    height: 25px;
}
.clsInit .btn{
    width: 120px;
    height: 35px;
    background-color: #ef4b0b;
    /*border: 1px;*/
    color: whitesmoke;
}
.divSS h5{
    margin-top: 15px;
    font-weight: normal;
}
.divMD{
    width: 100%;
    height: 750px;
    background-color: #ebebeb;
}
.divText{
    width: 50%;
    height: 130px;
    /*border: 1px solid red;*/
    float: left;
}
.divText h1{
    /*font-family: 宋体;*/
    font-size: 14px;
    color: #ef4b0b;
    margin-top: 15px;
    margin-left: 15px;
    margin-bottom: 15px;
    /*font-weight: normal;*/
}
.divText p{
    font-size: 12px;
    margin-left: 15px;

}